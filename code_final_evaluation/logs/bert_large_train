          (dropout_1): Dropout(p=0.1)
          (dropout_2): Dropout(p=0.1)
        )
        (layer_norm): LayerNorm(torch.Size([1024]), eps=1e-06, elementwise_affine=True)
        (dropout): Dropout(p=0.1)
      )
    )
    (dropout): Dropout(p=0.1)
    (layer_norm): LayerNorm(torch.Size([1024]), eps=1e-06, elementwise_affine=True)
    (wo): Linear(in_features=1024, out_features=1, bias=True)
    (sigmoid): Sigmoid()
  )
